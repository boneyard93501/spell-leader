// use std::path::Path;
use marine_rs_sdk::marine;
use marine_rs_sdk::module_manifest;

use curl_effector_types as curl;
use curl_effector_types::{CurlRequest, CurlResult};

module_manifest!();

pub fn main() {}

#[marine]
pub fn greeting(url: String) -> CurlResult {
    let my_request = CurlRequest {
        // url: "my_url".to_string(),
        url,
        headers: vec![],
        output_vault_path: "some path".to_string()
    };
    let res = curl::curl_get(my_request);
    // format!("curl result: {:?}", res)
    res
}
